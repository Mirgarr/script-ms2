--Mirgar ms2 gui script--

local library = loadstring(game:HttpGet('https://raw.githubusercontent.com/jakepscripts/moonlib/main/moonlibv1.lua'))()
local main = library:CreateWindow("Mirgar MS2 gui scripts", "#b36419", 9160626035)
main:CreateTab("Scripts")
main:CreateButton("Exodus", "Scripts", function()
do
    loadstring(game:HttpGet("https://system-exodus.com/scripts/MiningSimulator/MiningSimulator2.lua", true))()
end
    end)
    main:CreateButton("Zeerox", "Scripts", function()
        do
            loadstring(game:HttpGet'https://raw.githubusercontent.com/RunDTM/miningsim2/main/rewrite.lua')()", true))()"
        end
            end)
            main:CreateButton("Mirgar ms2 script", "Scripts", function()
                do
                    loadstring(game:HttpGet'https://raw.githubusercontent.com/Mirgarr/script-ms2/main/script')()", true))()"
                            end
                        end)
                    
                end)
